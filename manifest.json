{
  "name": "Google Index Checker",
  "description": "Index Checker",
  "version": "0.3",
  "manifest_version": 2,
  "permissions": ["tabs","storage"],
  "options_page": "options.html",
  "content_scripts": [{"matches": ["https://www.google.com/*"],"js": ["inject.js"],"run_at":"document_end","all_frames":true}]
}